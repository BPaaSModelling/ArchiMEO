speco:symptom_Bob
  rdf:type speco:Symptom ;
  speco:description "The symptom cough that Bob has." ;
  speco:symptom_Justifies_Isolation speco:Isolation_Of_Bob ;
  speco:symptom_Justifies_Test speco:Test_Of_Bob ;
  rdfs:label "Symptom Of Bob" ;
.
speco:symptom_Florian
  rdf:type speco:Symptom ;
  speco:symptom_Justifies_Isolation speco:Isolation_Of_Florian ;
  speco:symptom_Justifies_Test speco:Test_Of_Florian ;
  spin:query [
      rdf:type sp:Select ;
      sp:resultVariables (
          [
            sp:varName "subject" ;
          ]
        ) ;
      sp:where (
          [
            sp:object speco:Test_Of_Florian ;
            sp:predicate speco:symptom_Justifies_Test ;
            sp:subject [
                sp:varName "subject" ;
              ] ;
          ]
        ) ;
    ] ;
  rdfs:label "Symptom Of Florian" ;
.
